{
  "name": "@tonaiagent/core",
  "version": "1.1.0",
  "description": "Multi-provider AI layer with production-grade security, multi-agent coordination, strategy engine, plugin system, no-code visual builder, marketplace, copy trading, tokenomics, institutional compliance, omnichain infrastructure, and agent launchpad for autonomous agents on TON blockchain",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.mjs",
      "require": "./dist/ai/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.mjs",
      "require": "./dist/security/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.js"
    },
    "./multi-agent": {
      "import": "./dist/multi-agent/index.mjs",
      "require": "./dist/multi-agent/index.js",
      "types": "./dist/multi-agent/index.d.ts"
    },
    "./strategy": {
      "types": "./dist/strategy/index.d.ts",
      "import": "./dist/strategy/index.mjs",
      "require": "./dist/strategy/index.js"
    },
    "./no-code": {
      "types": "./dist/no-code/index.d.ts",
      "import": "./dist/no-code/index.mjs",
      "require": "./dist/no-code/index.js"
    },
    "./marketplace": {
      "types": "./dist/marketplace/index.d.ts",
      "import": "./dist/marketplace/index.mjs",
      "require": "./dist/marketplace/index.js"
    },
    "./tokenomics": {
      "types": "./dist/tokenomics/index.d.ts",
      "import": "./dist/tokenomics/index.mjs",
      "require": "./dist/tokenomics/index.js"
    },
    "./institutional": {
      "types": "./dist/institutional/index.d.ts",
      "import": "./dist/institutional/index.mjs",
      "require": "./dist/institutional/index.js"
    },
    "./omnichain": {
      "types": "./dist/omnichain/index.d.ts",
      "import": "./dist/omnichain/index.mjs",
      "require": "./dist/omnichain/index.js"
    },
    "./launchpad": {
      "types": "./dist/launchpad/index.d.ts",
      "import": "./dist/launchpad/index.mjs",
      "require": "./dist/launchpad/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/ai/index.ts src/security/index.ts src/plugins/index.ts src/multi-agent/index.ts src/strategy/index.ts src/no-code/index.ts src/marketplace/index.ts src/tokenomics/index.ts src/institutional/index.ts src/omnichain/index.ts src/launchpad/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/ai/index.ts src/security/index.ts src/plugins/index.ts src/multi-agent/index.ts src/strategy/index.ts src/no-code/index.ts src/marketplace/index.ts src/tokenomics/index.ts src/institutional/index.ts src/omnichain/index.ts src/launchpad/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "groq",
    "openai",
    "anthropic",
    "google",
    "xai",
    "openrouter",
    "multi-provider",
